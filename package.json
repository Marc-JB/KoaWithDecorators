{
    "name": "koa-with-decorators",
    "version": "0.1.2",
    "description": "Experimental library for using Koa with decorators",
    "private": false,
    "type": "commonjs",
    "main": "./build/main/index.js",
    "typings": "./build/main/index.d.ts",
    "scripts": {
        "build": "tsc -b ./src/main/",
        "lint": "eslint --ignore-path .gitignore .",
        "test": "tsc -b ./src/test/ && mocha --recursive --exit ./build/test/",
        "crossinstall": "npm install --package-lock-only && yarn install"
    },
    "engineStrict": true,
    "engines": {
        "node": ">= 10.9.0"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Marc-JB/KoaWithDecorators.git"
    },
    "author": "Marc",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/Marc-JB/KoaWithDecorators/issues"
    },
    "homepage": "https://github.com/Marc-JB/KoaWithDecorators#readme",
    "dependencies": {
        "@koa/router": "^9.1.0"
    },
    "devDependencies": {
        "@peregrine/test-with-decorators": "^0.1.5",
        "@types/chai": "^4.2.11",
        "@types/koa": "^2.11.3",
        "@types/koa__router": "^8.0.2",
        "@types/node": "^14.0.14",
        "@typescript-eslint/eslint-plugin": "^3.5.0",
        "@typescript-eslint/parser": "^3.5.0",
        "eslint": "^7.4.0",
        "koa": "^2.13.0",
        "typescript": "^3.9.6"
    }
}
